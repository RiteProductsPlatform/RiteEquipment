  "eventListeners": {
    "buttonAction": {
      "chains": [
        {
          "chain": "RemoveButtonActionChain",
          "parameters": {
            "key": "{{ $current.key }}",
            "index": "{{ $current.index }}",
            "current": "{{ $current }}"
          }
        }
      ]
    },
    "buttonAction2": {
      "chains": [
        {
          "chain": "PushButtonActionChain",
          "parameters": {
            "key": "{{ $current.key }}",
            "index": "{{ $current.index }}",
            "current": "{{ $current }}"
          }
        }
      ]
    },
    "buttonAction3": {
      "chains": [
        {
          "chain": "MapButtonActionChain"
        }
      ]
    },
    "buttonAction4": {
      "chains": [
        {
          "chain": "SaveButtonActionChain"
        }
      ]
    },
    "crewNameValueItem": {
      "chains": [
        {
          "chain": "CrewValueItemChangeChain",
