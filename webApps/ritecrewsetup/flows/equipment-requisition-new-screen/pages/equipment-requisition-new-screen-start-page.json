{
  "title": "equipment requisition new screen",
  "description": "",
  "variables": {
    "currindx": {
      "type": "number"
    },
    "equipmentnewADP": {
      "type": "vb/ArrayDataProvider2",
      "defaultValue": {
        "itemType": "postTestEQP_RequestPage",
        "keyAttributes": "equipment_id"
      }
    },
    "equpvariables": {
      "type": "postTestEQP_RequestPage"
    },
    "getGetCrewNameLOVListSDP": {
      "type": "vb/ServiceDataProvider",
      "defaultValue": {
        "endpoint": "TimeRite_Ords_Service/getGetCrewNameLOV",
        "keyAttributes": "crew_name",
        "itemsPath": "items",
        "responseType": "getGetCrewNameLOVResponse",
        "transformsContext": {
          "vb-textFilterAttributes": [
            "crew_name"
          ]
        }
      }
    },
    "getGetEquipmentClassLOVListSDP": {
      "type": "vb/ServiceDataProvider",
      "defaultValue": {
        "endpoint": "TimeRite_Ords_Service/getGetEquipmentClassLOV",
        "keyAttributes": "equipment_class",
        "itemsPath": "items",
        "responseType": "getGetEquipmentClassLOVResponse",
        "transformsContext": {
          "vb-textFilterAttributes": [
            "equipment_class"
          ]
        }
      }
    },
    "getGetEquipmentLOVListSDP": {
      "type": "vb/ServiceDataProvider",
      "defaultValue": {
        "endpoint": "TimeRite_Ords_Service/getGetEquipmentLOV",
        "keyAttributes": "lkpvalue",
        "itemsPath": "items",
        "responseType": "getGetEquipmentLOVResponse",
        "uriParameters": {
          "pClass": "{{ $variables.equpvariables.equipment_class }}"
        },
        "transformsContext": {
          "vb-textFilterAttributes": [
            "lkpvalue"
          ]
        }
      }
    },
    "getGetEquipmentLOVListSDP2": {
      "type": "vb/ServiceDataProvider",
      "defaultValue": {
        "endpoint": "TimeRite_Ords_Service/getGetEquipmentLOV",
        "keyAttributes": "lkpvalue",
        "itemsPath": "items",
        "responseType": "getGetEquipmentLOVResponse2",
        "uriParameters": {
          "pClass": "{{ $variables.equpvariables.equipment_class }}"
        },
        "transformsContext": {
          "vb-textFilterAttributes": [
            "lkpvalue"
          ]
        }
      }
    },
    "getTasksListSDP": {
      "type": "vb/ServiceDataProvider",
      "defaultValue": {
        "endpoint": "TimeRite_Ords_Service/getTasks",
        "keyAttributes": "task_name",
        "itemsPath": "items",
        "responseType": "getTasksResponse",
        "uriParameters": {
          "project_number": "{{ $variables.equpvariables.project_number }}"
        },
        "transformsContext": {
          "vb-textFilterAttributes": [
            "task_name"
          ]
        }
      }
    },
    "illustrationBackground": {
      "type": "string",
      "defaultValue": "https://static.oracle.com/cdn/fnd/gallery/2307.0.2/images/illust-welcome-banner-bg-01.png"
    },
    "illustrationForeground": {
      "type": "string",
      "defaultValue": "https://static.oracle.com/cdn/fnd/gallery/2307.0.2/images/illust-welcome-banner-fg-01.png"
    },
    "imageStretch": {
      "type": "string",
      "defaultValue": "none"
    },
    "mobileImage": {
      "type": "string",
      "defaultValue": "on"
    },
    "projectDetailsPWAListSDP": {
      "type": "vb/ServiceDataProvider",
      "defaultValue": {
        "endpoint": "TimeRite_Ords_Service/ProjectDetailsPWA",
        "keyAttributes": "project_name",
        "itemsPath": "items",
        "responseType": "projectDetailsPWAResponse",
        "transformsContext": {
          "vb-textFilterAttributes": [
            "project_name"
          ]
        }
      }
    },
    "equpvariables_copy": {
      "type": "postTestEQP_RequestPage"
    }

  },
  "metadata": {},
  "types": {
    "@decorators.getGetCrewNameLOVResponse": {
      "endpoint": "getGetCrewNameLOV",
      "service": "TimeRite_Ords_Service",
      "path": "response"
    },
    "@decorators.getGetEquipmentClassLOVResponse": {
