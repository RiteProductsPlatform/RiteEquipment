      "ast_insured_policy": "string",
      "ast_retired_date": "string",
      "auto_costing_flag": "string",
      "billing_category": "string",
      "city": "string",
      "cost_rate": "string",
      "country": "string",
      "county": "any",
      "created_by": "any",
      "creation_date": "string",
      "default_location": "string",
      "equipment_class": "string",
      "equipment_description": "string",
      "equipment_id": "number",
      "equipment_name": "string",
      "equipment_number": "string",
      "equipment_sub_class": "string",
      "equipment_type": "string",
      "expenditure_org_id": "any",
