      "mf_color": "any",
      "mf_fuel_type": "string",
      "mf_license_expiry": "string",
      "mf_license_number": "any",
      "mf_license_state": "string",
      "mf_model_number": "any",
      "mf_model_year": "number",
      "mf_serial_number": "string",
      "mf_warranty_code": "string",
      "mf_weight": "number",
      "non_labor_resource": "string",
      "non_labor_resource_org": "string",
      "organization_id": "any",
      "organization_name": "any",
      "owning_status": "any",
      "person_first_name": "any",
