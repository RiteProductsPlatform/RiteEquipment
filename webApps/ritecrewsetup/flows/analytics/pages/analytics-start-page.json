{
  "title": "analytics",
  "description": "",
  "variables": {
    "ChartADP": {
      "type": "vb/ArrayDataProvider2",
      "defaultValue": {
        "data": "{{ $variables.chartArray }}"
      }
    },
    "chartArray": {
      "type": "object[]"
    },
    "columns": {
      "type": "object[]"
    },
    "EquipmentClassLOVListSDP": {
      "type": "vb/ServiceDataProvider",
      "defaultValue": {
        "endpoint": "TimeRite_Ords_Service/getGetEquipmentClassLOV",
        "keyAttributes": "equipment_class",
        "itemsPath": "items",
        "responseType": "getGetEquipmentClassLOVResponse",
        "transformsContext": {
          "vb-textFilterAttributes": [
            "equipment_class"
          ]
        }
      }
    },
    "EquipmentTblADP": {
      "type": "vb/ArrayDataProvider2",
      "defaultValue": {
      }
    },
    "getGetEquipmentLOVListSDP": {
      "type": "vb/ServiceDataProvider",
      "defaultValue": {
        "endpoint": "TimeRite_Ords_Service/getGetEquipmentLOV",
        "keyAttributes": "lkpvalue",
        "itemsPath": "items",
        "responseType": "getGetEquipmentLOVResponse",
        "headers": {
          "pClass": "{{ $variables.searchObj.class }}"
        },
        "transformsContext": {
          "vb-textFilterAttributes": [
            "lkpvalue"
          ]
        },
        "uriParameters": {
          "pClass": "{{ $variables.searchObj.class }}"
        }
      }
    },
    "illustrationBackground": {
      "type": "string",
      "defaultValue": "https://static.oracle.com/cdn/fnd/gallery/2307.0.2/images/illust-welcome-banner-bg-01.png"
    },
    "illustrationForeground": {
      "type": "string",
      "defaultValue": "https://static.oracle.com/cdn/fnd/gallery/2307.0.2/images/illust-welcome-banner-fg-01.png"
    },
    "imageStretch": {
      "type": "string",
      "defaultValue": "none"
    },
    "mobileImage": {
      "type": "string",
      "defaultValue": "on"
    },
    "reportADP": {
      "type": "vb/ArrayDataProvider2",
      "defaultValue": {
        "data": "{{ $variables.reportsArray }}",
        "keyAttributes": "label"
      }
    },
    "reportsArray": {
      "type": "object[]",
      "defaultValue": [
        {
          "label": "Equipment Master"
        },
        {
          "label": "Equipment Utilization Report"
        },
        {
          "label": "Equipment Pending Approval Report"
        },
        {
          "label": "Equipment Onhand Quantity"
        }
      ]
    },
    "searchObj": {
      "type": {
        "class": "string",
        "from_date": "string",
        "name": "string",
        "report": "string",
        "to_date": "string"

      },
      "defaultValue": {
        "report": "Equipment Master"
      }
    }

  },
  "metadata": {},
  "types": {
    "@decorators.EquipmentType": {
      "endpoint": "getEquipmentMaster",
      "service": "TimeRite_Ords_Service",
      "path": "response-_-items[0]"
    },
    "@decorators.getGetEquipmentClassLOVResponse": {
      "endpoint": "getGetEquipmentClassLOV",
      "service": "TimeRite_Ords_Service",
      "path": "response"
    },
    "@decorators.getGetEquipmentLOVResponse": {
      "endpoint": "getGetEquipmentLOV",
      "service": "TimeRite_Ords_Service",
      "path": "response"
    },
    "chartType": {
      "id": "number",
      "series": "string",
      "group": "string",
      "value": "number"
    },
    "EquipmentType": {
      "address_line1": "any",
      "address_line2": "any",
      "asset_id": "any",
      "asset_number": "string",
      "asset_organization": "string",
      "ast_date_in_service": "string",
