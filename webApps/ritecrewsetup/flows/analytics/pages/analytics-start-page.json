      "record_mode": "any",
      "request_id": "any",
      "state": "any",
      "status": "string",
      "task_id": "number",
      "task_name": "string",
      "task_number": "any",
      "total_capacity_perday": "any",
      "vendor_id": "any",
      "vendor_name": "string",
      "zip": "any"
    },
    "getGetEquipmentClassLOVResponse": {
      "items": [
        {
          "equipment_class": "string"
        }
      ]
    },
    "getGetEquipmentLOVResponse": {
      "items": [
        {
          "lkpvalue": "string"
        }
      ]
    }
  },
  "eventListeners": {
    "buttonAction": {
      "chains": [
        {
          "chain": "SearchBtnAction"
        }
      ]
    },
    "buttonAction2": {
      "chains": [
        {
          "chain": "resetBtnAction"
        }
      ]
    },
    "buttonAction3": {
      "chains": [
        {
          "chain": "ExportBtnAction"
        }
      ]
    },
    "vbEnter": {
      "chains": [
        {
          "parameters": {},
          "chain": "SearchBtnAction"
        }
      ]
    }
  },
  "imports": {
    "components": {
      "oj-button": {
        "path": "ojs/ojbutton"
      },
      "oj-chart": {
        "path": "ojs/ojchart"
      },
      "oj-chart-item": {
        "path": "ojs/ojchart"
      },
      "oj-form-layout": {
        "path": "ojs/ojformlayout"
      },
      "oj-input-date": {
        "path": "ojs/ojdatetimepicker"
      },
      "oj-select-single": {
        "path": "ojs/ojselectsingle"
      },
      "oj-sp-welcome-page": {
        "path": "oj-sp/welcome-page/loader"
      },
      "oj-table": {
