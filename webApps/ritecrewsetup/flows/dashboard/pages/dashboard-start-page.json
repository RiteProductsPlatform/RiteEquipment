{
  "title": "Dashboard",
  "description": "",
  "variables": {
    "illustrationBackground": {
      "type": "string",
      "defaultValue": "https://static.oracle.com/cdn/fnd/gallery/2307.0.2/images/illust-welcome-banner-bg-01.png"
    },
    "illustrationForeground": {
      "type": "string",
      "defaultValue": "https://static.oracle.com/cdn/fnd/gallery/2307.0.2/images/illust-welcome-banner-fg-01.png"
    },
    "imageStretch": {
      "type": "string",
      "defaultValue": "none"
    },
    "jwttoken": {
      "type": "string"
    },
    "mobileImage": {
      "type": "string",
      "defaultValue": "on"
    },
    "actionsBarVisible": {
      "type": "boolean",
      "defaultValue": false
    },
    "chartData": {
      "type": "vb/ArrayDataProvider2",
      "defaultValue": {
        "data": "{{ $variables.chartDataArray }}",
        "itemType": "ChartType",
        "keyAttributes": "id"
      }
    },
    "chartDataArray": {
      "type": "ChartType[]",
      "defaultValue": [
        {
          "id": 1,
          "series": "Quotes Generated",
          "group": "Group 1",
          "value": 10
        },
        {
          "id": 2,
          "series": "Quotes Requested",
          "group": "Group 1",
          "value": 15
        },
        {
          "id": 3,
          "series": "Contract",
          "group": "Group 1",
          "value": 25
        }
      ]
    },
    "notificationData": {
      "type": "vb/ArrayDataProvider2",
      "defaultValue": {
        "data": "{{ $variables.notificationDataArray }}",
        "itemType": "NotificationType",
        "keyAttributes": "id"
      }
    },
    "notificationDataArray": {
      "type": "NotificationType[]",
      "defaultValue": [
        {
          "id": 1,
          "name": "Project Equipment Requested",
          "value": 75,
          "badge": 1
        },
        {
          "id": 1,
          "name": "Customer Equipment Requested",
          "value": 3,
          "badge": 1
        },
        {
          "id": 1,
          "name": "Quotes Accepted",
          "value": 12,
          "badge": 2
        }
      ]
    }

  },
  "metadata": {},
  "types": {
    "ChartType": {
      "group": "string",
      "id": "number",
      "series": "string",
      "value": "number"
    },
    "NotificationType": {
      "id": "number",
      "name": "string",
      "value": "number",
      "badge": "number"
    }
  },
  "eventListeners": {
    "buttonAction": {
      "chains": [
        {
          "chain": "ButtonActionChain"
        }
      ]
    },
    "flexClick": {
      "chains": [
        {
          "chain": "FlexClickChain"
        }
      ]
    },
    "flexClick2": {
      "chains": [
        {
          "chain": "FlexClickChain1"
        }
      ]
    },
    "flexClick3": {
      "chains": [
        {
          "chain": "FlexClickChain2"
        }
      ]
    },
    "spanClick": {
      "chains": [
        {
          "chain": "spanClickChain"
        }
      ]
    }

  },
  "imports": {
    "components": {
      "oj-button": {
        "path": "ojs/ojbutton"
      },
      "oj-chart": {
        "path": "ojs/ojchart"
      },
      "oj-chart-item": {
        "path": "ojs/ojchart"
      },
      "oj-list-item-layout": {
        "path": "ojs/ojlistitemlayout"
      },
      "oj-list-view": {
        "path": "ojs/ojlistview"
      },
      "oj-sp-welcome-page": {
        "path": "oj-sp/welcome-page/loader"
      }
    }
  }
}